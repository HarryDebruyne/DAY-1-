#pragma warning(disable:4996)// 옛날 방식으로 짜도 돌아가게 함
#include<stdio.h>
get_num()
{
	return 5;
 }

max_num(int c, int d)
{
	if (c < d)
	{
		return d;

	}
	else if (c > d)
	{
		return c;
	}
	else if (c = d)
		return 0;

}

put_max(int a)
{
	printf("%d", a);
}
main()
{
	int a, b;
	a = get_num();
	b = get_num();
	int max;
	max = max_num(a,b);
	put_max(max);
}
