int get_num()  // int 나가는 통로 get_num함수명 ()
{
	int b = 40;
	return b;
	//return 40;
}

main()
{
	int a = 10;
	printf("%d\n", a);
	
	a = 20;
	printf("%d\n", a);

	scanf("%d", &a); //표준입력함수를 사요하여 a 의 주소에 키보드로 십진수를 넣는다~
	printf("%d\n", a);
	
	a = get_num();
	printf("%d\n", a);
}

