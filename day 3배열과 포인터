
#include<stdio.h>

main()
{
	int a;
	scanf("%d", &a);

	int* p;
	p = &a;
	
	scanf("%d", p);

	printf("%d\n", *p);
}



void put_num(int a)

{
	a = 10;
}

main()
{
	int a;
	put_num(a);
	printf("%d\n", a);
}
